  "name": "github-issue-rank",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "async": "~1.4.2",
    "bootstrap": "^3.3.5",
    "cors": "^2.7.1",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "font-awesome": "^4.4.0",
    "griddle": "~0.1.2",
    "griddle-react": "~0.2.14",
    "history": "^1.12.5",
    "lodash": "~3.10.1",
    "oauthio-web": "^0.4.4",
    "object-assign": "^4.0.1",
    "octokat": "^0.4.12",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.1",
    "react-dom": "^0.14.0",
    "react-loader": "^2.0.0",
    "react-router": "~1.0.0-rc1",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "~6.3.0",
    "browserify": "~11.2.0",
    "debowerify": "~1.3.1",
    "npm-run-all": "^1.2.12",
    "watchify": "~3.4.0"
  },
  "scripts": {
    "test": "node build/test.bundle.js",
    "build": "npm-run-all build:**:*",
    "build:src:cp:public": "cp -r src/* public",
    "build:src:cp:node": "mkdir -p public/node_modules/font-awesome && cp -r node_modules/font-awesome public/node_modules",
    "build:src:css": "lessc src/less/index.less public/css/index.css",
    "build:src:brfy": "browserify --debug src/browser.js -t babelify -t debowerify --outfile public/browser.bundle.js",
    "build:test:brfy": "browserify --debug test/index.js -t babelify -t debowerify --outfile build/test.bundle.js",
    "watch": "npm-run-all --parallel watch:**:*",
    "watch:src": "watchify --debug src/browser.js -t babelify -t debowerify --outfile public/browser.bundle.js",
    "watch:test": "watchify --debug test/index.js -t babelify -t debowerify --outfile build/test.bundle.js"
  },
  "author": "Anders D. Johnson",
