    "test": "test"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "octokat": "^0.4.12",
    "react": "^0.13.3",
    "fixed-data-table": "~0.4.7",
    "async": "~1.4.2",
    "griddle": "~0.1.2",
    "underscore": "~1.8.3",
    "griddle-react": "~0.2.14",
